####### Build rules

all: Makefile $(TARGET)

$(TARGET):  $(UICDECLS) $(OBJECTS) $(OBJMOC)  
	test -d ../ || mkdir -p ../
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJMOC) $(OBJCOMP) $(LIBS)

mocables: $(SRCMOC)
uicables: $(UICDECLS) $(UICIMPLS)

$(MOC): 
	( cd $(QTDIR)/src/moc && $(MAKE) )

Makefile: vec2web.pro  /usr/share/qt3/mkspecs/default/qmake.conf /usr/share/qt3/lib/libqt-mt.prl
	$(QMAKE) -o Makefile vec2web.pro
qmake: 
	@$(QMAKE) -o Makefile vec2web.pro

dist: 
	@mkdir -p obj/vec2web && $(COPY_FILE) --parents $(SOURCES) $(HEADERS) $(FORMS) $(DIST) obj/vec2web/ && ( cd `dirname obj/vec2web` && $(TAR) vec2web.tar vec2web && $(GZIP) vec2web.tar ) && $(MOVE) `dirname obj/vec2web`/vec2web.tar.gz . && $(DEL_FILE) -r obj/vec2web

